{
  "name": "Space Donut Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*.readonly": "#e8b2f8",
    "builtinConstant": "#8283df"
  },
  "colors": {
    "focusBorder": "#e1d8dc",
    "foreground": "#cccccc",
    "widget.shadow": "#14151A",
    "selection.background": "#e1d8dc",
    "errorForeground": "#FC644D",

    "button.background": "#3A6395",
    "button.hoverBackground": "#3A6395",

    "dropdown.background": "#22262f",
    "dropdown.border": "#363c49",

    "input.background": "#22262f",
    "input.placeholderForeground": "#e1d8dc",
    "inputOption.activeBorder": "#C668BA",
    "inputValidation.errorBackground": "#D65343",
    "inputValidation.errorBorder": "#D65343",
    "inputValidation.infoBackground": "#3A6395",
    "inputValidation.infoBorder": "#3A6395",
    "inputValidation.warningBackground": "#DE9237",
    "inputValidation.warningBorder": "#DE9237",

    "scrollbar.shadow": "#0c111b",
    "scrollbarSlider.activeBackground": "#3A3F4CCC",
    "scrollbarSlider.background": "#3A3F4C77",
    "scrollbarSlider.hoverBackground": "#3A3F4CAA",

    "badge.background": "#98a6d4",
    "badge.foreground": "#10151f",

    "progressBar.background": "#C668BA",

    "list.activeSelectionBackground": "#0c111b",
    "list.activeSelectionForeground": "#6890c2",
    "list.dropBackground": "#3a404e",
    "list.focusBackground": "#282b35",
    "list.focusForeground": "#eee",
    "list.hoverBackground": "#0c111b",
    "list.hoverForeground": "#eee",
    "list.inactiveSelectionBackground": "#0c111b",
    "list.inactiveSelectionForeground": "#98a6d4",

    "activityBar.background": "#070b11",
    "activityBar.foreground": "#BAAFC0",
    "activityBarBadge.background": "#98a6d4",
    "activityBarBadge.foreground": "#10151f",
    "activityBar.border": "#070b11",

    "sideBar.background": "#070b11",
    "sideBarSectionHeader.background": "#070b11",
    "sideBarTitle.foreground": "#98a6d4",
    "sideBar.foreground": "#e1d8dc",
    "sideBar.border": "#070b11",

    "editorGroup.dropBackground": "#495061d7",
    "editorGroupHeader.tabsBackground": "#070b11",

    "tab.activeBackground": "#131a2a",
    "tab.inactiveBackground": "#070b11",
    "tab.activeForeground": "#98a6d4",
    "tab.inactiveForeground": "#e1d8dc",
    "tab.border": "#070b11",

    "editor.background": "#070b11",
    "editor.foreground": "#cccccc",
    "editorLineNumber.foreground": "#e1d8dc",
    "editorCursor.foreground": "#cccccc",
    "editor.selectionBackground": "#3D4352",
    "editor.selectionHighlightBackground": "#4F435580",
    "editor.wordHighlightBackground": "#2d2730",
    "editor.wordHighlightStrongBackground": "#505050",
    "editor.findMatchBackground": "#91704cd0",
    "editor.findMatchHighlightBackground": "#5f5f5fd7",
    "editor.hoverHighlightBackground": "#27282e",
    "editor.lineHighlightBackground": "#17191f",
    "editor.lineHighlightBorder": "#17191f",
    "editorLink.activeForeground": "#3B79C7",
    "editor.rangeHighlightBackground": "#372F3C",
    "editorWhitespace.foreground": "#333844",
    "editorIndentGuide.background1": "#333844",
    "editorIndentGuide.activeBackground1": "#585C66",
    "editorRuler.foreground": "#4F4355",
    "editorCodeLens.foreground": "#e1d8dc",
    "editorBracketMatch.background": "#4f4e4f",
    "editorBracketMatch.border": "#2f2f30",
    "editorOverviewRuler.border": "#0c111b",
    "editorError.foreground": "#ff0035",
    "editorWarning.foreground": "#cafe48",
    "editorGutter.modifiedBackground": "#5BC0EBBB",
    "editorGutter.addedBackground": "#9BC53DBB",
    "editorGutter.deletedBackground": "#FC644DBB",

    "diffEditor.insertedTextBackground": "#29BF1220",
    "diffEditor.removedTextBackground": "#F21B3F20",

    "editorWidget.background": "#20232A",
    "editorSuggestWidget.background": "#20232A",
    "editorSuggestWidget.border": "#372F3C",
    "editorSuggestWidget.selectedBackground": "#27282e",
    "editorHoverWidget.background": "#27282e",
    "editorHoverWidget.border": "#98a6d4",
    "debugExceptionWidget.background": "#FF9F2E60",
    "debugExceptionWidget.border": "#FF9F2E60",

    "peekView.border": "#0c111b",
    "peekViewEditor.background": "#1A1C22",
    "peekViewEditor.matchHighlightBackground": "#FF9F2E60",
    "peekViewResult.background": "#1A1C22",
    "peekViewResult.matchHighlightBackground": "#FF9F2E60",
    "peekViewResult.selectionBackground": "#0c111b",
    "peekViewTitle.background": "#1A1C22",
    "peekViewTitleDescription.foreground": "#e1d8dc",

    "merge.currentHeaderBackground": "#F92672",
    "merge.currentContentBackground": "#F9267240",
    "merge.incomingHeaderBackground": "#3B79C7BB",
    "merge.incomingContentBackground": "#3B79C740",

    "panel.background": "#070b11",
    "panel.border": "#070b11",
    "panelTitle.activeBorder": "#2054bb",
    "panelTitle.inactiveForeground": "#e1d8dc",

    "statusBar.background": "#070b11",
    "statusBar.debuggingBackground": "#FC644D",
    "statusBar.noFolderBackground": "#070b11",
    "statusBarItem.activeBackground": "#98a6d4",
    "statusBarItem.hoverBackground": "#98a6d4",
    "statusBarItem.prominentBackground": "#98a6d4",
    "statusBarItem.prominentHoverBackground": "#98a6d4",

    "gitDecoration.untrackedResourceForeground": "#8bcf7d",
    "gitDecoration.modifiedResourceForeground": "#e5c744",

    "list.warningForeground": "#d18b65",

    "terminal.ansiRed": "#ee5d43",
    "terminal.ansiGreen": "#96E072",
    "terminal.ansiYellow": "#FFE66D",
    "terminal.ansiBlue": "#7cb7ff",
    "terminal.ansiMagenta": "#ff00aa",
    "terminal.ansiCyan": "#00e8c6",
    "terminal.ansiBrightRed": "#ee5d43",
    "terminal.ansiBrightGreen": "#96E072",
    "terminal.ansiBrightYellow": "#eccb26",
    "terminal.ansiBrightBlue": "#7cb7ff",
    "terminal.ansiBrightMagenta": "#ff00aa",
    "terminal.ansiBrightCyan": "#00e8c6",
    "terminalCursor.background": "#0c111b",
    "terminalCursor.foreground": "#98a6d4",

    "titleBar.activeBackground": "#0c111b",
    "titleBar.inactiveBackground": "#10151f",

    "editorBracketHighlight.foreground1": "#0070ad",
    "editorBracketHighlight.foreground2": "#ffad00",
    "editorBracketHighlight.foreground3": "#DA62A1",

    "extensionButton.prominentBackground": "#3A6395",
    "extensionButton.prominentHoverBackground": "#30527c",

    "pickerGroup.border": "#4F4355",
    "pickerGroup.foreground": "#e1d8dc",

    "debugToolBar.background": "#20232A",

    "walkThrough.embeddedEditorBackground": "#0c111b"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Comment color",
      "scope": [
        "comment",
        "markup.quote.markdown",
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#5f6167"
      }
    },
    {
      "name": "Text Color",
      "scope": [
        "meta.template.expression.js",
        "constant.name.attribute.tag.jade",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.separator",
        "punctuation.section.function.begin"
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Cyan",
      "scope": [
        "variable.parameter",
        "support.variable",
        "entity.name.tag.yaml",
        "constant.character.entity.html",
        "source.css entity.name.tag.reference",
        "beginning.punctuation.definition.list.markdown",
        "source.css entity.other.attribute-name.parent-selector",
        "meta.structure.dictionary.json support.type.property-name",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#70c8d9"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "markup.bold",
        "constant.numeric",
        "meta.group.regexp",
        "constant.other.php",
        "support.constant.ext.php",
        "constant.other.class.php",
        "support.constant.core.php",
        "fenced_code.block.language",
        "constant.other.caps.python",
        "entity.other.attribute-name",
        "support.type.exception.python",
        "source.css keyword.other.unit",
        "variable.other.object.property.js.jsx",
        "variable.other.object.js",
        "storage"
      ],
      "settings": {
        "foreground": "#8af0ca"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "markup.list",
        "text.xml string",
        "entity.name.type",
        "support.function.declaration",
        "entity.other.attribute-name",
        "meta.at-rule.extend",
        "entity.name.function.declaration",
        "entity.other.inherited-class",
        "entity.other.keyframe-offset.css",
        "text.html.markdown string.quoted",
        "meta.function-call.generic.python",
        "meta.at-rule.extend support.constant",
        "entity.other.attribute-name.class.jade",
        "source.css entity.other.attribute-name",
        "text.xml punctuation.definition.string",
        "meta.function-call.generic",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#fa8be3"
      }
    },
    {
      "name": "Pink",
      "scope": [
        "markup.heading",
        "variable.language.this.js",
        "variable.language.special.self.python",
        "keyword.control.import",
        "punctuation.definition.interpolation",
        "punctuation.section.embedded.end.php",
        "punctuation.section.embedded.end.ruby",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.begin.ruby",
        "punctuation.definition.template-expression",
        "entity.name.tag",
        "meta.function"
      ],
      "settings": {
        "foreground": "#befec3"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "keyword",
        "meta.link",
        "meta.image",
        "markup.italic",
        "source.js support.type"
      ],
      "settings": {
        "foreground": "#00b069"
      }
    },
    {
      "name": "Blue",
      "scope": ["string.regexp", "markup.changed"],
      "settings": {
        "foreground": "#c9b48f"
      }
    },
    {
      "name": "Red",
      "scope": [
        "constant",
        "support.class",
        "keyword.operator",
        "support.constant",
        "text.html.markdown string",
        "source.css support.function",
        "source.php support.function",
        "support.function.magic.python",
        "entity.other.attribute-name.id",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#d7f7b5"
      }
    },
    {
      "name": "Green",
      "scope": [
        "string",
        "text.html.php string",
        "markup.inline.raw",
        "markup.inserted",
        "punctuation.definition.markdown",
        "text.html meta.embedded source.js string",
        "text.html.php punctuation.definition.string",
        "text.html meta.embedded source.js punctuation.definition.string",
        "text.html punctuation.definition.string",
        "text.html string"
      ],
      "settings": {
        "foreground": "#da627d"
      }
    },
    {
      "name": "Font Underline",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Font Italic",
      "scope": [
        "comment",
        "invalid",
        "keyword.control",
        "entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
